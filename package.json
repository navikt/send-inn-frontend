    "@sentry/nextjs": "^7.99.0",
    "axios": "^1.6.7",
    "html-react-parser": "^5.1.1",
    "jose": "^5.2.0",
    "next": "^14.1.0",
    "openid-client": "^5.6.4",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "react-i18next": "^14.0.1",
    "sanitize-html": "^2.11.0",
    "styled-components": "^6.1.8",
    "swr": "^2.2.4",
    "use-debounce": "^10.0.0",
    "uuid": "^9.0.1",

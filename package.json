    "@sentry/nextjs": "^7.73.0",
    "axios": "^1.5.1",
    "html-react-parser": "^4.2.2",
    "jose": "^4.15.2",
    "next": "13.5.4",
    "openid-client": "^5.6.0",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^13.2.2",
    "sanitize-html": "^2.11.0",
    "styled-components": "^6.0.8",
    "swr": "^2.2.4",

{
  "name": "send-inn-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3100",
    "dev:mock": "NODE_ENV=test next dev -p 3100",
    "build": "next build",
    "build:mock": "NODE_ENV=test next build",
    "build:static": "next build && next export",

{
  "name": "send-inn-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3100",
    "dev:mock": "cross-env-shell NODE_ENV=test next dev -p 3100",
    "build": "next build",
    "build:mock": "cross-env-shell NODE_ENV=test next build",
    "build:static": "next build && next export",
    "start": "next start -p 3100",
    "start:with:test": "cross-env-shell NODE_ENV=test next start -p 3100",
    "lint": "eslint .",
    "prepare": "husky",
    "cypress": "npx cypress open",
    "cypress:headless": "npx cypress run",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0",
    "@navikt/aksel-icons": "^7.31.0",
    "@navikt/ds-css": "^7.31.0",
    "@navikt/ds-react": "^7.31.0",
    "@navikt/nav-dekoratoren-moduler": "^3.4.0",
    "@sentry/nextjs": "^8.55.0",
    "axios": "^1.12.2",
    "html-react-parser": "^5.2.6",
    "next": "^16.0.1",
    "prom-client": "^15.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-i18next": "^16.2.3",
    "sanitize-html": "^2.17.0",
    "styled-components": "^6.1.19",
    "swr": "^2.3.6",
    "use-debounce": "^10.0.6",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@sentry/cli": "^2.56.0",
    "@testing-library/cypress": "^10.1.0",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.2",
    "@types/sanitize-html": "^2.16.0",
    "@types/uuid": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.31.0",
    "cross-env": "^10.1.0",
    "cypress": "^15.3.0",
    "cypress-axe": "^1.7.0",
    "eslint": "^9.37.0",
    "eslint-config-next": "^15.5.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "msw": "^2.11.3",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,mdx}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.0.0"
  }
}

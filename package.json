    "@typescript-eslint/parser": "^6.7.4",
    "cypress": "^13.3.0",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "msw": "^1.3.2",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.2.2"

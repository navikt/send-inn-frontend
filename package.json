    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "cypress": "^13.6.4",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.1",
    "msw": "^2.1.5",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "typescript": "^5.3.3"
